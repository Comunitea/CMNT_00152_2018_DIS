<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <template id="website_base_website_assets_frontend" inherit_id="website.assets_frontend">
        <xpath expr="link[last()]" position="after">
            <!-- Theme Dismac -->
            <link href="/website_base/static/css/portal.less" rel="stylesheet" type="text/less"/>
        </xpath>
        <!--Add custom scripts-->
        <xpath expr="script[last()]" position="after">
            <script type="text/javascript" src="/website_base/static/js/my_portal_history.js" />
            <script type="text/javascript" src="/website_base/static/js/website_sale.js" />
            <script type="text/javascript" src="/website_base/static/js/website_sale_comparison.js" />
            <script type="text/javascript" src="/website_base/static/js/website_sale_options.js" />
        </xpath>
    </template>
    
</odoo>