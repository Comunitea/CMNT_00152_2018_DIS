<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <template id="website_logo_add_logo_to_website" inherit_id="website_logo.add_logo_to_website" name="Show website Logo Base">
        <xpath expr="//header//a[hasclass('navbar-brand', 'logo')][2]" position="after">
            <a href="http://www.dismacpruebas.es.mialias.net" class="navbar-brand logo">
                <img class="img img-responsive" src="/website_dismac/static/img/dismac_logo_footer.png" alt="website.name"/>
            </a>
        </xpath>
        <xpath expr="//header//a[hasclass('navbar-brand', 'logo')][2]" position="attributes">
            <attribute name="class" add="d-none" separator=" "/>
        </xpath>
    </template>

</odoo>