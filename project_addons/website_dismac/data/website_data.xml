<?xml version="1.0" encoding="utf-8" ?>
<odoo>
        <data>

            <!--Create Website Dismac-->
            <record id="dismac_website" model="website">
                <field name="name">Dismac</field>
                <field name="domain">dismac</field>
                <field name="canonical_domain">http://dismac</field>
                <field name="favicon" type="base64" file="website_dismac/static/img/dismac_favicon_1.png"/>
                <field name="logo" type="base64" file="website_dismac/static/img/dismac_logo_web.png"/>
                <field name="social_youtube">https://www.youtube.com/channel/UCUtHfBLaOOs1f1nFtCKVr5g/</field>
                <field name="social_facebook">https://www.facebook.com/DISMACMOBILIARIO</field>
                <field name="social_linkedin">https://www.linkedin.com/company/dismac</field>
                <field name="social_default_image" type="base64" file="website_dismac/static/img/dismac_logo_web.png"/>
                <field name="base_url">http://dismac</field>
                <field name="specific_user_account">True</field>
                <field name="auth_signup_uninvited">b2c</field>
                <field name="show_line_subtotals_tax_selection">tax_included</field>
                <field name="group_activity_statement">True</field>
                <field name="default_aging_type">days</field>
                <field name="email">info@dismac.es</field>
            </record>

        </data>
</odoo>